                                                        minOccurs = "0" nillable="true"/>
      <xsd:element name = "Description"                 type = "DescriptionType"         
                                                        minOccurs = "0" maxOccurs = "unbounded"  nillable="true"/>
      <xsd:element name = "HierarchyScope"              type = "HierarchyScopeType"    
                                                        minOccurs = "0"  nillable="true"/>
      <xsd:element name = "PublishedDate"               type = "DateTimeType" 
                                                        minOccurs = "0"  nillable="true"/>
      <xsd:element name = "OperationsEventClass"        type = "OperationsEventClassType"      
                                                        minOccurs = "0" maxOccurs = "unbounded"  nillable="true"/>
      <xsd:element name = "OperationsEventDefinition"   type = "OperationsEventDefinitionType" 
                                                        minOccurs = "0" maxOccurs = "unbounded"  nillable="true"/>
      <xsd:element name = "OperationsEvent"             type = "OperationsEventType"        
                                                        minOccurs = "0" maxOccurs = "unbounded" nillable="true"/>
      <xsd:group   ref  = "Extended:OperationsEventInformation" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

