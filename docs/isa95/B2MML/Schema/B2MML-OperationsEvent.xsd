      <xsd:element name = "HierarchyScope"               type = "HierarchyScopeType"   
                                                         minOccurs = "0" />
      <xsd:element name = "Extension"            		 type = "IdentifierType"   
                                                         minOccurs = "0" maxOccurs = "unbounded"/>
      <xsd:element name = "OperationsEventClass"         type = "OperationsEventClassType" 
	                                                     minOccurs = "0" maxOccurs = "unbounded"/> <!-- "Is assembled from" relationship -->
      <xsd:element name = "UsesPropertiesOfOperationsEventClassID"
                                                         type = "IdentifierType"
                                                         minOccurs = "0" maxOccurs = "unbounded"/> <!-- "Includes properties of" relationship -->
      <xsd:element name = "OperationsEventClassProperty" type = "OperationsEventClassPropertyType"
                                                         minOccurs = "0" maxOccurs = "unbounded"/> <!-- "Has properties of" relationship -->
      <xsd:element name = "OperationsEventDefinitionID"  type = "IdentifierType"
                                                         minOccurs = "0" maxOccurs = "unbounded"/> <!-- Is parent of relationship to event definitions -->
      <xsd:element name = "RecordSpecification" 		 type = "OperationsRecordSpecTemplateType"      
                                                         minOccurs = "0" maxOccurs = "unbounded"/> <!-- "Has record specifications of" relationship --> 
      <xsd:group   ref  = "Extended:OperationsEventClass"      minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

